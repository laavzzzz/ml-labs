   "source": [
    "import numpy as np\n",
    "\n",
    "data = np.array([10, 15, 20, 50, 60])\n",
    "x = 20\n",
    "\n",
    "mean = np.mean(data)\n",
    "std = np.std(data, ddof=0)   # population std\n",
    "\n",
    "z = (x - mean) / std\n",
    "print(\"Z-score of 20:\", z)\n"
   ]
