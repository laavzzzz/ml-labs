      "Bins: [array([ 5, 10, 11, 12]), array([15, 35, 50, 55]), array([ 72,  92, 204, 215])]\n",
      "Smoothed by bin means: [9, 9, 9, 9, 38, 38, 38, 38, 145, 145, 145, 145]\n"
     ]
    }
   ],
   "source": [
    "import numpy as np\n",
    "\n",
    "data = np.array([5,10,11,12,15,35,50,55,72,92,204,215])\n",
    "bins = np.array_split(data, 3)\n",
    "\n",
    "smoothed = []\n",
    "for b in bins:\n",
    "    mean = int(np.mean(b))\n",
    "    smoothed.extend([mean] * len(b))\n",
    "\n",
    "print(\"Bins:\", bins)\n",
    "print(\"Smoothed by bin means:\", smoothed)\n"
   ]
  },
